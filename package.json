{
  "name": "cfye-nextjs",
  "version": "1.0.2",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "serve": "next build && next start",
    "generate:codegen": "DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config",
    "generate:codegen:watch": "graphql-codegen -w -r dotenv/config",
    "lint": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn lint",
      "yarn prettier"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "eslint": "^7.11.0",
    "framer-motion": "^4.1.2",
    "html-react-parser": "^1.4.0",
    "next": "^11.1.2",
    "next-sitemap": "^1.6.192",
    "postcss": "^8.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-inview": "^4.3.11",
    "react-intersection-observer": "^8.32.1",
    "sass": "^1.43.2",
    "string-strip-html": "^8.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-resolvers": "^2.3.2",
    "@types/node": "^15.14.9",
    "@types/react": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dotenv": "^8.2.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "graphql": "^15.6.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.4.4"
  },
  "license": "MIT"
}
