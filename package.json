{
  "name": "wp-headless-client-nextjs",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "serve": "next build && next start",
    "generate:codegen": "DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config",
    "generate:codegen:watch": "graphql-codegen -w -r dotenv/config",
    "lint": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn lint",
      "yarn prettier"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.16.1",
    "eslint": "^7.11.0",
    "framer-motion": "^4.1.2",
    "html-react-parser": "^1.2.4",
    "next": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.29.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/node": "^14.11.8",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "graphql": "^15.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.0.3"
  },
  "license": "MIT"
}
